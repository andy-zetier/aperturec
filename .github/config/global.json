{
  "registry": "ghcr.io",
  "environment": {
    "RUST_BACKTRACE": "1",
    "CARGO_TERM_COLOR": "always"
  },
  "rust": {
    "default_toolchain": "stable",
    "components": {
      "format": "rustfmt",
      "lint": "clippy",
      "docs": "rust-docs"
    },
    "cargo_packages": {
      "audit": "cargo-deny"
    },
    "cache": {
      "cache_on_failure": true,
      "prefix_key_version": "v0"
    }
  },
  "tools": {
    "go_version": "1.22"
  }
}
{
  "crate_groups": [
    {
      "name": "client",
      "crates": ["aperturec-client-gtk3", "aperturec-client"],
      "package_crate": "aperturec-client-gtk3",
      "package_types": ["dmg", "tar.gz"],
      "runner": "macos-15",
      "needs_display": false
    }
  ],
  "jobs": {
    "package": {
      "artifacts": {
        "client": {
          "name": "client-package",
          "paths": [
            "aperturec-client-macos*.tar.gz",
            "aperturec-client-macos-with-all-dependencies_*.zip"
          ]
        }
      }
    }
  }
}